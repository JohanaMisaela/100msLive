{
  "name": "zipyai",
  "version": "1.4.5",
  "description": "SDK for zipy",
  "source": "src/zipy.ts",
  "main": "dist/zipy.js",
  "module": "dist/zipy.esm.js",
  "types": "dist/zipy.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "microbundle --sourcemap false && npm run microbundle-standalone",
    "microbundle-standalone": "microbundle -o dist/zipy.min.js --external none --format umd --sourcemap false",
    "un-compress:microbundle": "microbundle -o dist/zipy.min.js --external none --format umd --sourcemap false --compress false",
    "dev": "microbundle watch",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "Zipy",
    "sdk",
    "for",
    "recording",
    "sessions"
  ],
  "exports": {
    ".": {
      "import": "./dist/zipy.esm.js",
      "require": "./dist/zipy.js",
      "default": "./dist/zipy.js"
    },
    "./package.json": "./package.json"
  },
  "author": "Zipy",
  "license": "ISC",
  "dependencies": {
    "uuid": "^8.3.2",
    "zipy-rrweb": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@types/jest": "^27.0.3",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "microbundle": "^0.13.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.1.3"
  }
}
